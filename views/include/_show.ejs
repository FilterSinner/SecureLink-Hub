<% for(var i=0;i < users.length; i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].id %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].department %></td>
    <td>
        <a href="/update-user?ids=<%= users[i]._id%>" class="btn border update">
        <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border delete" data-id=<%= users[i]._id%>>
            <span class="text-gradient"><i class="fas fa-trash"></i></span>
        </a>
    </td>
</tr>
<% } %>
